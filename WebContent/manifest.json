{
  "name": "PageArchiver",
  "icons": {
    "16": "resources/icon_16.png",
    "48": "resources/icon_48.png",
    "128": "resources/icon_128.png" },
  "version": "0.1.19",
  "description": "Helps you to archive web pages for offline reading",
  "background": {
    "scripts": [
      "scripts/migration.js",
      "scripts/zip.js",
      "scripts/background.js",
      "scripts/storage.js"
    ]
  },
  "options_page": "pages/options.html",
  "browser_action": {
    "default_icon": "resources/icon_48.png",
    "default_title": "View archives",
    "default_popup": "pages/popup.html"
  },
  "permissions" : ["tabs", "unlimitedStorage", "notifications"],
  "minimum_chrome_version" : "7",
  "omnibox" : {
    "keyword" : "pa"
  },
  "web_accessible_resources": [
    "resources/icon_48.png",
    "pages/proxy-page.css",
    "scripts/color-picker.js",
    "scripts/proxy-page.js"
  ],
  "manifest_version": 2
}